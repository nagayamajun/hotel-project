<h1>お問合せ</h1>
<%= form_with model: @contact do |form| %>
<p>
  <%= form.label :name %><br>
  <%= form.text_field :name %>
</p>

<p>
  <%= form.label :email %><br>
  <%= form.text_field :email %>
</p>

<p>
  <%= form.label :body %><br>
  <%= form.text_area :body%>
</p>

<div>
  <%= form.submit %>
</div>
<% end %>
